# Default workspace (for analyst mode)
DATABRICKS_SERVER_HOSTNAME=https://your-workspace.cloud.databricks.com
DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/your-warehouse-id
DATABRICKS_TOKEN=dapi_your_token_here

# Optional: Production workspace (for developer mode)
PRODUCTION_DATABRICKS_SERVER_HOSTNAME=https://prod.cloud.databricks.com
PRODUCTION_DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/prod-id
PRODUCTION_DATABRICKS_TOKEN=dapi_prod_token

# Optional: Development workspace  (for developer mode)
DEV_DATABRICKS_SERVER_HOSTNAME=https://dev.cloud.databricks.com
DEV_DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/dev-id
DEV_DATABRICKS_TOKEN=dapi_dev_token

# Optional: Staging workspace (for developer mode)
STAGING_DATABRICKS_SERVER_HOSTNAME=https://staging.cloud.databricks.com
STAGING_DATABRICKS_HTTP_PATH=/sql/1.0/warehouses/staging-id
STAGING_DATABRICKS_TOKEN=dapi_staging_token

# Optional: Default catalog and schema for UDF functions
DATABRICKS_DEFAULT_CATALOG=your_catalog
DATABRICKS_DEFAULT_SCHEMA=your_schema
